<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane nodeOrientation="LEFT_TO_RIGHT" prefHeight="500.0" prefWidth="600.0" styleClass="rootCenterPane" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="springboot_ar.chat.client.gui.controller.Login">
   <children>
      <VBox alignment="CENTER" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox alignment="CENTER" maxHeight="50.0" maxWidth="500.0" minHeight="50.0" minWidth="500.0" prefHeight="50.0" prefWidth="500.0" spacing="30.0" styleClass="rootCenterPane_box">
               <children>
                  <Label alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="SpringBoot_ar Chat Client JavaFx" VBox.vgrow="ALWAYS">
                     <font>
                        <Font name="System Bold Italic" size="26.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <VBox alignment="CENTER" maxHeight="350.0" maxWidth="500.0" minHeight="350.0" minWidth="500.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="350.0" prefWidth="500.0" spacing="30.0" styleClass="rootCenterPane_box">
               <children>
                  <HBox maxWidth="1.7976931348623157E308" spacing="10.0">
                     <children>
                        <TextField fx:id="txtUsername" alignment="CENTER" maxWidth="1.7976931348623157E308" styleClass="textUsername" HBox.hgrow="ALWAYS">
                           <font>
                              <Font name="System Bold Italic" size="26.0" />
                           </font>
                        </TextField>
                        <Label alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="160.0" minWidth="160.0" prefWidth="160.0" text="HostName">
                           <font>
                              <Font name="System Bold Italic" size="26.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox maxWidth="1.7976931348623157E308" spacing="10.0">
                     <children>
                        <TextField fx:id="txtUsername1" alignment="CENTER" maxWidth="1.7976931348623157E308" styleClass="textUsername" HBox.hgrow="ALWAYS">
                           <font>
                              <Font name="System Bold Italic" size="26.0" />
                           </font>
                        </TextField>
                        <Label alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="160.0" minWidth="160.0" prefWidth="160.0" text="UserName">
                           <font>
                              <Font name="System Bold Italic" size="26.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox maxWidth="1.7976931348623157E308" spacing="10.0">
                     <children>
                        <TextField fx:id="txtUsername2" alignment="CENTER" maxWidth="1.7976931348623157E308" styleClass="textUsername" HBox.hgrow="ALWAYS">
                           <font>
                              <Font name="System Bold Italic" size="26.0" />
                           </font>
                        </TextField>
                        <Label alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="160.0" minWidth="160.0" prefWidth="160.0" text="ChatPass">
                           <font>
                              <Font name="System Bold Italic" size="26.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <Button maxWidth="1.7976931348623157E308" onAction="#onClickLogin" styleClass="textUsername" text="Login">
                     <font>
                        <Font name="System Bold Italic" size="26.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
